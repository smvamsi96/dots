set number

syntax on

set hlsearch

set tabstop=4

set incsearch

set shiftwidth=4

set showmatch

set clipboard=unnamed

filetype on

set autoindent

map <F8> <Esc>=%<Esc>

abbreviate ab abbreviate

abbreviate hed #include<stdio.h> int main() {  }

abbreviate smv elliot.jsteve

abbreviate #b /*********

abbreviate #e *********/

abbreviate im I'm

abbreviate al alias
